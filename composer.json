{
    "name": "dvandersluijs/wsdl-to-class",
    "description": "Generate PHP7 code from an WSDL file",
    "license": "GPL-3.0-or-later",
    "keywords": [ "soap", "soap-wsdl", "code-generation", "wsdl", "generator", "php" ],
    "authors": [
        {
            "name": "Danny van der Sluijs",
            "email": "danny.vandersluijs@icloud.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "nikic/php-parser": "^4.1",
        "symfony/console": "^4.1",
        "twig/twig": "^2.5",
        "ext-soap": "*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.3",
        "mikey179/vfsStream": "^1.6",
        "phpunit/phpunit": "^7.4"
    },
    "bin": [
        "wsdltoclass"
    ],
    "autoload": {
        "psr-4": {
            "WsdlToClass\\": "src",
            "WsdlToClassTest\\": "test"
        }
    },
    "scripts": {
        "cs-check": "phpcs --standard=PSR2 src test",
        "cs-fix": "phpcbf --standard=PSR2 src test",
        "test": [
            "curl -L https://phar.phpunit.de/phpunit-7.phar -o build/phpunit.phar",
            "chmod +x build/phpunit.phar",
            "./build/phpunit.phar --dump-xdebug-filter build/xdebug-filter.php",
            "./build/phpunit.phar --prepend build/xdebug-filter.php --coverage-clover=coverage.clover"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
