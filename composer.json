{
    "name": "dvandersluijs/wsdl-to-class",
    "description": "Import a WSDL to output classes",
    "license": "GNU GENERAL PUBLIC LICENSE",
    "keywords": [ "soap", "soap-wsdl", "code-generation" ],
    "authors": [
        {
            "name": "Danny van der Sluijs",
            "email": "danny.vandersluijs@icloud.com"
        }
    ],
    "require": {
        "cilex/cilex": "^2.0",
        "twig/twig": "~1.24",
        "php": "^5.6|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "phpunit/phpunit-mock-objects": "2.3.*",
        "zendframework/zend-cache": "2.2.10",
        "symfony/filesystem": "2.8.3",
        "symfony/translation": "2.8.3",
        "symfony/yaml": "2.8.3",
        "squizlabs/php_codesniffer": "^2.8"
    },
    "bin": [
        "wsdltoclass"
    ],
    "autoload": {
        "psr-4": { "WsdlToClass\\": "src/WsdlToClass" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.5-dev"
        }
    }
}
